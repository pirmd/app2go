'---+ Use common environment --------------------------------------------------
LoadScriptIfExists "include\cygwin.2go" 'rsync if available

'---+ Who  and Where am I? -----------------------------------------------------
App = "Unison"
Ver = "-2.40.63"
LoadScript "include\app2goFSH.2go"

'---+ Tweak environment to be portable ----------------------------------------
CLArgs = "-log=false " & CLArgs
SetEnv "UNISON", CfgFolder

'---+ Run Main App ------------------------------------------------------------
Run Exe, CLArgs, RUN_NORMAL, WAIT_FOR_ME


